      },
      {
        "caption": "Governance",
        "link": "https://gov.openswap.xyz/"
      }
    ],
    "socialMediaInfo": [
      {
        "img": "{root}/assets/img/footer/twitter-darktheme.svg",
        "link": "https://twitter.com/openswapdex/"
      },
      {
        "img": "{root}/assets/img/footer/medium-darktheme.svg",
        "link": "https://medium.com/@openswapdex/"
      },
      {
        "img": "{root}/assets/img/footer/linkedin-darktheme.svg",
